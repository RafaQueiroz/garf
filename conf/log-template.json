{
  "template" : "honeyd",
  "version" : 5001,
  "settings" : {
    "index.refresh_interval" : "5s"
  },
  "mappings" : {
    "_default_" : {
      "properties" : {
        "access_date": {
          "type": "date"
        },
        "connection_status": {
          "type": "text"
        },
        "destination_port": {
          "type": "keyword"
        },
        "protocol": {
          "type": "keyword"
        },
        "source_ip": {
          "type": "keyword"
        }
      }
    }
  }
}

